<mjml>
  <mj-head>
    <mj-title>@Model.Sender.FirstName @Model.Sender.LastName has uploaded the following files to @model.Matter.FileNo</mj-title>
    <mj-include path="../../partials/head-lawconnect.mjml" />
  </mj-head>
  <mj-include path="../../partials/body-start.mjml" />
  <!-- Header -->
    <header theme="lawconnect"/>
  <!-- Header -->
  <!-- Message -->
    <message>
      <p>Hi <strong>{FirstName} {LastName}</strong>,</p>
      <p>
        @Model.Sender.FirstName @Model.Sender.LastName from @Model.FirmName <strong>has uploaded </strong>the following files to @model.Matter.FileNo.
      </p>
    </message>
  <!-- End of Message -->
  <!-- List Component -->
  <mj-include path="../../components/list/start.mjml" />
    <list-item icon="doc" name="Document-Title.extention" showBtn="true" btnText="View" link="#"/>
    <list-item icon="pdf" name="Document-Title.extention" showBtn="true" btnText="View" link="#"/>
    <list-item icon="folder" name="Document-Title.extention" showBtn="true" btnText="View" link="#"/>
  <mj-include path="../../components/list/end.mjml" />
  <!-- End of List Component -->
  <!-- Footer -->
    <footer theme="lawconnect" />
  <!-- End of Footer -->
<mj-include path="../../partials/body-end.mjml" />
